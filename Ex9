
package Lec4;
import java.util.Scanner;
public class Ex9 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Enter a integer: ");
        int x = scanner.nextInt();
        int gttd = (x>0) ? x : x*-1;
        System.out.println("The absolute value is: "+ gttd);
    }
}
